.done-priorities {
	display: none;
	flex-wrap: wrap;
	margin: 40px 0;
	padding: 0;
	border: 10px solid white;
	justify-content: space-between;

	&__item {
		flex-grow: 1;
		flex-basis: 30%;
		min-width: 240px;
		width: 30%;
		padding: 40px;
		font-size: 22px;
		line-height: 28px;
		font-weight: 100;
		border: 10px solid white;
	}
}
